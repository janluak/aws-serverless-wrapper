{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CachedTableForTests",
  "description": "short table for testing cached loading items",
  "type": "object",
  "additionalProperties": false,
  "default": [
    "primary_partition_key"
  ],
  "properties": {
    "primary_partition_key": {
      "description": "the primary partition key of the dynamo db",
      "type": "string"
    },
    "some_string": {
      "description": "some key containing any kind of strings",
      "type": "string",
      "enum": ["abcdef", "ghijkl", "NewBee"]
    },
    "some_int": {
      "description": "some key containing any integers",
      "type": "integer"
    },
    "some_float": {
      "description": "some key containing any floats",
      "type": "number"
    }
  },
  "required": [
    "primary_partition_key",
    "some_string",
    "some_int",
    "some_float"
  ]
}